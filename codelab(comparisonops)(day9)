a = 1
b = 2
c = 3

#== equal to
var_e = (a == b)
var_e1 = (b == c)
var_e2 = (c == a)
print(var_e)
print(var_e1)
print(var_e2)

#> greater than
var_g = (a > b)
var_g1 = (b > c)
var_g2 = (c > a)
print(var_g)
print(var_g1)
print(var_g2)
#< less than
var_l = (a < b)
var_l1 = (b < c)
var_l2 = (c < a)
print(var_l)
print(var_l1)
print(var_l2)

# != not equal to
var_n = (a != b)
var_n1 = (b != c)
var_n2 = (c != a)
print(var_n)
print(var_n1)
print(var_n2)
